      "type": "richtext",
      "required": true
    },
    "rating": {
      "type": "integer",
      "required": true,
      "default": 0
    },
    "categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "inversedBy": "articles"
    }
  }
}
